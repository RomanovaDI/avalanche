         transportModel  Newtonian;
         nu         1e-06;
         rho        1000;
     }

     soil
     {
         transportModel HerschelBulkley;
         HerschelBulkleyCoeffs
         {
             nu0   [ 0 2 -1 0 0 0 0 ] 1e-03;
             tau0  [ 0 2 -2 0 0 0 0 ] 1e-06;
             k     [ 0 2 -1 0 0 0 0 ] 1e-04;
             n     [ 0 0  0 0 0 0 0 ] 2;
         }
         //transportModel  Newtonian;
         //nu         1e-06;
         rho        2000;
     }

     gas
     {
         transportModel  Newtonian;
         nu         1.48e-05;
         rho        1;
     }
);

sigmas
(
    (gas liquid) 0.0
    (gas soil) 0.0
    (gas entrained) 0.0
    (liquid soil) 0.0
    (liquid entrained) 0.0

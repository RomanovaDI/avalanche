         //    tau0  [ 0 2 -2 0 0 0 0 ] 1e-03;//1e-06;
         //    k     [ 0 2 -1 0 0 0 0 ] 4e-06;
         //    n     [ 0 0  0 0 0 0 0 ] 0.7;

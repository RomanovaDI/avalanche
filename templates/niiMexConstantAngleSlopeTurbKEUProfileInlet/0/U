FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
	leftInletWall
	{
		type			timeVaryingMappedFixedValue;
//		value			uniform (0.5 0 0);
	}
	leftWall
	{
		type			noSlip;
	}
	rightWall
	{
		type			inletOutlet;
		inletValue		uniform (0 0 0);
	}
	lowerWall
	{
		type			noSlip;
	}
	atmosphere
	{
		type			pressureInletOutletVelocity;
		value			uniform (0 0 0);
	}
	sides
	{
		type			slip;
	}
	defaultFaces
	{
		type			empty;
	}
}


// ************************************************************************* //

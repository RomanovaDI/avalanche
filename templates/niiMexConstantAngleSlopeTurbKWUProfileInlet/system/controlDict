FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

//application     tbnnTurbulenceInterFoam;
application     interFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1;

//deltaT          5e-06;
deltaT          1e-03;

writeControl    adjustable;

writeInterval   1e-02;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           1;
maxAlphaCo      1;

maxDeltaT       1;

